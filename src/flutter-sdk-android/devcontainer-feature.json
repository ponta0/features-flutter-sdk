{
    "name": "Flutter SDK Android Components",
    "id": "flutter-sdk-android",
    "version": "0.1.0",
    "description": "Install the Flutter SDK Android Components",
    "containerEnv": {
        "ANDROID_HOME": "/usr/lib/android-sdk",
        "PATH": "$PATH:$ANDROID_HOME/cmdline-tools/latest/bin:$ANDROID_HOME/tools/bin:$ANDROID_HOME/platform-tools/bin"
    },
    "postStartCommand": "flutter doctor --android-licenses"
}
